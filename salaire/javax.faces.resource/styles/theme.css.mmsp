.ui-menu.ui-menubar {
	padding-right: 20px !important;
	padding-left: 10px !important;
	background-color: #ffffff !important;
	border: 1px solid #ffffff !important;
	background-image: linear-gradient(to bottom, #ffffff, #ffffff);
	/* background-repeat: repeat-x; */
	/* border: 1px solid #d4d4d4; */
}

.ui-state-default, .ui-widget-content .ui-state-default,
	.ui-widget-header .ui-state-default {
	text-shadow: none !important;
}

.error-template {
	padding: 40px 15px;
	text-align: center;
}

.error-actions {
	margin-top: 15px;
	margin-bottom: 15px;
}

.error-actions .btn {
	margin-right: 10px;
}

.interruption {
	background-color: #FFB5B5 !important;
	background-image: none !important;
	color: #000000 !important;
}

.result {
	color: #0067ce !important;
}

.resultSalaire {
	color: #36b09c !important;
    font-weight: bold;
}

.indication {
	color: #b03636 !important;
	line-height: 25px !important;
}

.msgInformatif {
	color: #36b09c !important;
	line-height: 25px !important;
}

.tooltipContent {
	color: #337ab7 !important;
	font-size: 11.5px !important;
	line-height: 20px !important;
	font-family: 'Comic Sans MS', cursive, sans-serif !important
}

.tooltipMessage {
	color: #b03636 !important;
	font-size: 11.5px !important;
	line-height: 20px !important;
	font-family: 'Comic Sans MS', cursive, sans-serif !important
}

.intervalMessage {
	color: #006699 !important;
	font-size: 12.5px !important;
	line-height: 25px !important;
	font-family: 'Comic Sans MS', cursive, sans-serif !important
}

.total {
	background-color: aliceblue !important;
}

.ui-selectonemenu {
	min-width: auto !important;
}

.explicationSimulation {
	color: #1DA3FF !important;
	font-size: 12.5px !important;
	line-height: 25px !important;
	font-family: 'Comic Sans MS', cursive, sans-serif !important
}

#motifPanelGrid, #donneesPersonnellesPanel, #dateRadiationPanel,
	#situationAdministrativeSortieId, #mutuelleAllocationFamilialePanel {
	max-width: 82vw !important;
}

#calculSalaireReferencePanel {
	max-width: 82vw !important;
}

/* afficher un * en rouge devant les champs obligatoires */
.required {
	background-image: url(/salaire/javax.faces.resource/general/required.png.mmsp?ln=images);
	background-repeat: no-repeat;
	background-position: right top;
	padding-right: 10px;
	white-space: nowrap;
}