@CHARSET "ISO-8859-1";


/* l'image de fond de l'entete */
.head_corps {
	background: url(/salaire/javax.faces.resource/general/bg_menu.gif.mmsp?ln=images) repeat-x;
}

/* l'image de fond de l'entete */
.menu_top {
	background: url(/salaire/javax.faces.resource/general/bg_header.jpg.mmsp?ln=images);
}

.TopMenu {
    background-color: var(--bg-color, #f1ba3f);
    height: 55px;
}

.TopMenu a {
	font: 10px Tahoma, Arial, Helvetica, Sans-serif !important;
	font-weight: normal !important;
	color: #0069B1;
	text-decoration: none;
	font-weight: normal;
}

.TopMenu a:hover {
	color: #A5BAC8;
	text-decoration: none;
}

.TopMenu span {
	margin: 0 3px;
}

/* l'image fond du pied centrale */
.footer_corps {
	background: url(/salaire/javax.faces.resource/general/footer_corps.png.mmsp?ln=images)
		repeat-x;
}

/* l'image fond du pied droite */
.footer_right {
	background: url(/salaire/javax.faces.resource/general/footer_right.png.mmsp?ln=images) 100% 0
		no-repeat;
}

/* l'image fond du pied gauche */
.footer_left {
	background: url(/salaire/javax.faces.resource/general/footer_left.png.mmsp?ln=images)
		no-repeat;
}

/* couleur du font de copyright */
.copyright {
	color: white;
}

/* style du conteneur de l'annimation de progression ajax 
.progress-box {
	border-bottom: 1px solid #ccc;
	border-top: 1px solid #ccc;
	background: #f0f0f0;
	height: 22px;
	vertical-align: middle;
}
*/
.ui-inputfield, .ui-widget-content .ui-inputfield, .ui-widget-header .ui-inputfield
	{
	background: white;
	-moz-box-shadow: inset 0 0px 0px #8f8f8f;
	-webkit-box-shadow: inset 0 1px 1px #8F8F8F;
	box-shadow: inset 0 1px 1px #8F8F8F;
}

